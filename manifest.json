 
{
   "name": "Redmine Activities",
   "description": "Get notified of new activities on Redmine",
   "version": "0.1",
   "manifest_version": 2,
   "permissions": [ "tabs", "notifications", "storage", "<all_urls>" ],
   "icons": {
      "16": "img/logo_16.png",
      "48": "img/logo_48.png",
      "128": "img/logo_128.png"
   },
   "background": {
      "scripts" : [ "js/lib/jquery-1.10.2.min.js", "js/lib/moment.min.js", "js/background.js"]
   },
   "browser_action": {
      "default_icon" : { 
         "19":  "img/logo_19.png",
         "38":  "img/logo_38.png"
      },
      "default_title": "Redmine Activities",
      "default_popup": "popup.html"
   },
   "options_page": "options.html"
}
